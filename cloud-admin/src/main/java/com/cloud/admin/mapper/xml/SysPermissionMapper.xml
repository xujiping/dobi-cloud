<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.admin.mapper.SysPermissionMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cloud.admin.entity.SysPermission">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="platform_id" property="platformId" />
        <result column="menu_name" property="menuName" />
        <result column="menu_value" property="menuValue" />
        <result column="up_id" property="upId" />
        <result column="level" property="level" />
        <result column="uri" property="uri" />
        <result column="status" property="status" />
    </resultMap>

</mapper>
