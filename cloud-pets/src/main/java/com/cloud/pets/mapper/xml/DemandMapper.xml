<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.pets.mapper.DemandMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cloud.pets.entity.Demand">
        <id column="id" property="id" />
        <result column="type_id" property="typeId" />
        <result column="type_name" property="typeName" />
        <result column="pub_time" property="pubTime" />
        <result column="user_id" property="userId" />
        <result column="pets_id" property="petsId"/>
        <result column="age" property="age" />
        <result column="price" property="price" />
        <result column="sex" property="sex" />
        <result column="category_id" property="categoryId" />
        <result column="species_id" property="speciesId" />
        <result column="content" property="content" />
        <result column="status" property="status" />
    </resultMap>

</mapper>
